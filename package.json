{
  "name": "@nowarp/blueprint-misti",
  "version": "0.3.0",
  "description": "Bluerpint Misti Plugin",
  "bugs": "https://github.com/nowarp/blueprint-misti/issues",
  "main": "dist/index.js",
  "author": "Georgiy Komarov <jubnzv@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nowarp/blueprint-misti.git"
  },
  "keywords": [
    "static analysis",
    "misti",
    "ton",
    "blockchain",
    "smart-contracts",
    "blueprint"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "fmt": "prettier --write src",
    "release": "yarn release-it"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "^5.0.0",
    "@ton/blueprint": "^0.22.0",
    "@ton/core": "^0.53.0",
    "@ton/crypto": "^3.2.0",
    "@ton/ton": "^13.9.0",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.4",
    "@types/node": "^20.2.5",
    "prettier": "^3.2.5",
    "release-it": "^17.6.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@ton/blueprint": ">=0.12.0",
    "@ton/core": ">=0.49.2",
    "@ton/crypto": ">=3.2.0",
    "@ton/ton": ">=13.4.1"
  },
  "dependencies": {
    "@babel/core": "^7.23.6",
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.23.6",
    "@nowarp/misti": "^0.3.0",
    "arg": "^5.0.2"
  },
  "prettier": {
    "semi": true,
    "printWidth": 80
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "hooks": {
      "before:release": "yarn clean && yarn build",
      "after:bump": "yarn build"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md",
        "addUnreleased": true
      }
    }
  },
  "packageManager": "yarn@1.22.22"
}
